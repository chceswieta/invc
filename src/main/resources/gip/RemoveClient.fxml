<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gip.RemoveClientController">
    <Label alignment="CENTER" focusTraversable="true" layoutX="45.0" layoutY="30.0" prefWidth="210.0" style="&#10;" text="Remove a client" textAlignment="CENTER" textFill="dimgray">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <TextField fx:id="nip" layoutX="65.0" layoutY="80.0" prefHeight="25.0" prefWidth="170.0" promptText="NIP" />

    <Button layoutX="65.0" layoutY="120.0" onAction="#removeClient" prefHeight="25.0" prefWidth="170.0" text="Remove" />
    <Label fx:id="info" alignment="CENTER" layoutX="45.0" layoutY="155.0" prefHeight="25.0" prefWidth="210.0" textFill="dimgray" wrapText="true" />
</AnchorPane>
